{
    "name": "ts-arrange",
    "displayName": "ts-arrange",
    "description": "TypeScript code anrrangement",
    "version": "0.0.1",
    "publisher": "DrMueller",
    "engines": {
        "vscode": "^1.10.0"
    },
    "files.associations": {
        "*.ts": "ts"
    },
    "homepage": "https://github.com/DrMueller/TS-Arrange2",
    "repository": {
        "type": "git",
        "url": "https://github.com/DrMueller/TS-Arrange2"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.arrangeFile",
                "title": "Arrange File"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "lodash": "^4.17.4",
        "sinon": "^2.1.0"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.61",
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "@types/sinon": "2.1.2",
        "mocha": "^2.3.3",
        "typescript": "2.5.3",
        "vscode": "^1.0.0"
    },
    "icon": "assets/T.PNG"
}